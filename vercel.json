{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/build/$1"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://your-backend-domain.vercel.app/api"
  },
  "functions": {
    "frontend/package.json": {
      "includeFiles": "frontend/build/**"
    }
  }
}
